{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-restricted-globals": "off",
    "no-alert": "off"
  }
}
